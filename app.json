{
  "expo": {
    "name": "FireChat",
    "slug": "MyFireChat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/FireChat.png",
    "scheme": "myfirechat",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anusorn.s.MyFireChat",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#e53935",
        "foregroundImage": "./assets/images/FireChat.png"
      },
      "notification": {
        "icon": "./assets/images/FireChat.png",
        "color": "#e53935"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anusorn.s.MyFireChat",
      "softwareKeyboardLayoutMode": "resize"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/FireChat.png",
          "imageWidth": 220,
          "resizeMode": "contain",
          "backgroundColor": "#e53935",
          "dark": {
            "backgroundColor": "#e53935"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0431042f-4621-42d6-bd86-228056e538f3"
      },
